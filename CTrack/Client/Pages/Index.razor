@page "/"

@using System.Linq


<PageTitle>Index</PageTitle>

<div class="content">
    <h1>Hello, world!</h1>

    <span >Welcome to your new app.</span>

    <SurveyPrompt Title="EEEEEEE" />


    <CustomAuthRedirect>
        <span>If you see this, you are authorized, good job!</span>
        <br />
        <span>
            @foreach (var x in context.User.Identities)
            {
                @x.Name
            }
        </span>
    </CustomAuthRedirect>
</div>